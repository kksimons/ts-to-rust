{
  "name": "forge-cli",
  "version": "0.1.0",
  "description": "TypeScript-to-Rust API generator CLI that creates production-ready backends from TypeScript declarations",
  "keywords": ["typescript", "rust", "cli", "api", "generator", "backend", "axum", "diesel"],
  "author": "kksimons",
  "license": "MIT",
  "homepage": "https://github.com/kksimons/ts-to-rust#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kksimons/ts-to-rust.git"
  },
  "bugs": {
    "url": "https://github.com/kksimons/ts-to-rust/issues"
  },
  "main": "dist/index.js",
  "bin": {
    "forge": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.1.1",
    "ts-morph": "^20.0.0",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.2",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ]
}